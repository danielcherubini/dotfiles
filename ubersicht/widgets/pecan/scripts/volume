#!/bin/bash

VOLUME=$(osascript -e 'set ovol to output volume of (get volume settings)')
ICON="ðŸ”ˆ"

if [ "$VOLUME" = "missing value" ]; then
	VOLUME=0
	ICON="ðŸ”‡"
fi

if (( $VOLUME > 66 )); then
	ICON="ðŸ”Š"
elif (( $VOLUME > 33 )); then
	ICON="ðŸ”‰"
elif (( $VOLUME > 0 )); then
	ICON="ðŸ”ˆ"
else
	ICON="ðŸ”‡"
fi

echo "$ICON $VOLUME"
